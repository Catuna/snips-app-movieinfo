{
  "version": 1,
  "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
  "handlers": {
    "console": {
      "formatter": "simple",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "MovieTriviaApp": {
      "level": "INFO",
      "handlers": [
        "console"
      ]
    }
  }

}
